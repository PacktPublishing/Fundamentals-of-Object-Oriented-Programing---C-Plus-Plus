#include <iostream>

using namespace std;


/* Functions
 *
*/

//function prototype
void starline();
void customline(char, int);

int main()
{
    //function call
    //starline();
    customline('=', 20);
    cout << "Gas        Price\n";
    //starline();
    customline('=', 20);
    cout << "Unleaded    $4.58" << endl
        <<  "Premium     $6.15" << endl
        <<  "Diesel      $5.73" << endl;
    //starline();
    customline('=', 20);

    return 0;
}

//function definition
void starline(){
    for (int i=0; i<40; i++)
        cout << '*';
    cout << endl;
}

void customline(char myChar, int n){
    for (int n=0; n<40; n++)
        cout << myChar;
    cout << endl;
}
