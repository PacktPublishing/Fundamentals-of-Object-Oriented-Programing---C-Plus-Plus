#include <iostream>

using namespace std;


/* Loops: For
 *
*/

int main()
{
//    for(int i=0; i<15; i++){
//        cout << i*i << " ";
//        cout << endl;
//    } //end for loop

    for(int num=1; num<=10; num++){ //loop from 1 - 10
        cout << setw(4) << num; //display 1st column
        int cube = num*num*num; //calculate the cube
        cout << setw(6) << cube << endl; //display 2nd colum
    }

    return 0;
}
