#include <iostream>

using namespace std;


/* Relational & Logical Operators
 * (==, !=, <, >, <=, >= )
 * (!, &&, ||)
*/

int main()
{
//    cout << !(8 == 6) << '\n';
//    cout << !(8 > 9) << '\n';
//    cout <<(4 != 5) << '\n';
//    cout <<(8 <= 10) << '\n';
//    cout <<(4 > 2) << '\n';

    cout << ((7 == 7) && (4 > 8)) << endl;
    cout << ((7 == 7) || (4 > 8)) << endl;


    return 0;
}
